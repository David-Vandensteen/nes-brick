{
  "name": "nes-brick",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "start:hello": "npm-run-all download:tools build:hello",
    "build:hello": "npm run start -- src/project/hello/config.mjs",
    "download:tools": "npx -y make-dir-cli %temp%/nes-brick && npx -y download-cli --extract --out %temp%/nes-brick https://downloads.sourceforge.net/project/cc65/cc65-snapshot-win32.zip && npx -y download-cli --extract --out %temp%/nes-brick https://downloads.sourceforge.net/project/fceultra/Binaries/2.6.4/fceux-2.6.4-win32.zip",
    "lint": "eslint src",
    "test:build-and-run": "node test/build-and-run.js",
    "test:asm-writer": "node test/asmWriter.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/David-Vandensteen/nes-brick.git"
  },
  "author": "David Vandensteen <dvandensteen@gmail.com>",
  "license": "MIT",
  "imports": {
    "#src/*": "./src/*.js",
    "#test/*": "./src/*.js"
  },
  "bugs": {
    "url": "https://github.com/David-Vandensteen/nes-brick/issues"
  },
  "homepage": "https://github.com/David-Vandensteen/nes-brick#readme",
  "devDependencies": {
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "make-dir-cli": "^3.0.0",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "uid": "^2.0.0"
  }
